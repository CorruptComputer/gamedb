{{define "_social"}}

    <div class="social ml-1">
        <div class="fb-like" data-href="https://gamedb.online{{ .GetPath }}" data-layout="button" data-action="like" data-size="large" data-show-faces="false" data-share="false"></div>
    </div>
    <div class="social ml-1">
        <a class="twitter-share-button" href="https://twitter.com/intent/tweet?text={{ .GetName }}" data-url="https://gamedb.online{{ .GetPath }}" data-size="large"></a>
    </div>

{{end}}
